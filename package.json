{
  "name": "@linkedmink/multilevel-aging-cache-workspace",
  "version": "0.5.1",
  "description": "Package provides an interface to cache and persist data to Redis, MongoDB, memory",
  "keywords": [
    "cache",
    "memory",
    "redis",
    "mongodb",
    "database",
    "storage",
    "distributed"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan@linkedmink.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/multilevel-aging-cache"
  },
  "private": true,
  "scripts": {
    "start": "ts-node-dev --inspect --respawn --transpile-only demo/index.ts",
    "build": "yarn workspaces foreach -pt run build",
    "test": "yarn workspaces foreach -pt run test",
    "lint": "yarn workspaces foreach -pt run lint",
    "doc": "yarn workspaces -pt run doc",
    "postinstall": "husky install || cd .",
    "pack:all": "yarn workspaces foreach -pt --from \"{main,plugins/*}\" run publish",
    "publish:all": "yarn workspaces foreach -pt --from \"{main,plugins/*}\" run publish",
    "publish:ioredis": "cd plugins/ioredis && npm publish --access public",
    "publish:mongodb": "cd plugins/mongodb && npm publish --access public",
    "publish:mongoose": "cd plugins/mongoose && npm publish --access public"
  },
  "optionalDependencies": {
    "@linkedmink/multilevel-aging-cache": "workspace:*",
    "@linkedmink/multilevel-aging-cache-ioredis": "workspace:*",
    "@linkedmink/multilevel-aging-cache-mongoose": "workspace:*",
    "@tsconfig/node16": "^1.0.2",
    "@types/ioredis": "^4.28.3",
    "husky": "^7.0.4",
    "ioredis": "^4.28.2",
    "lint-staged": "^12.1.2",
    "mongoose": "^5.13.13",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.3"
  },
  "workspaces": [
    "main",
    "plugins/ioredis",
    "plugins/mongodb",
    "plugins/mongoose"
  ],
  "packageManager": "yarn@3.1.1"
}
